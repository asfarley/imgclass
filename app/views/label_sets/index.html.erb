<h2>Listing Label Sets</h2>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @label_sets.each do |label_set| %>
      <tr>
        <td><%= link_to 'Show', label_set %></td>
        <td><%= link_to 'Edit', edit_label_set_path(label_set) %></td>
        <td><%= link_to 'Destroy', label_set, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Label set', new_label_set_path %>
