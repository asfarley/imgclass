<div class="container">
<h1>Jobs</h1>

<table class="table">
  <thead>
    <tr>
      <th>Image label set</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jobs.each do |job| %>
      <tr>
        <td><%= job.image_label_set %></td>
        <td><%= job.user.email %></td>
        <td><%= link_to 'Start Labeling', job %></td>
      </tr>
    <% end %>
  </tbody>
</table>

</div>
