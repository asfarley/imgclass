<h1>Listing Image Sets</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @image_sets.each do |image_set| %>
      <tr>
        <td><%= link_to 'Show', image_set %></td>
        <td><%= link_to 'Edit', edit_image_set_path(image_set) %></td>
        <td><%= link_to 'Destroy', image_set, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Image set', new_image_set_path %>
